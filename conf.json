{
  "source": {
    "include" : ["dist/js/hawkular-ui-components.js"]
  },
  "opts": {
    "template": "node_modules/angular-jsdoc/angular-template",
    "destination": "dist/docs",
    "readme": "README.adoc",
    "recourse": true
  },
  "plugins": [
    "node_modules/jsdoc/plugins/markdown",
    "node_modules/angular-jsdoc/common/plugins/ngdoc"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
